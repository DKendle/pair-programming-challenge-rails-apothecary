<h1>Treatments</h1>

<table id="treatments-table">
  <% @treatments.each do |treatment| %>

  <tr>
    <td><%= link_to treatment.name, treatment %></td>
    <td><%= treatment.description %></td>
    <td><%= button_to "Prescribe", '#', disabled: true %></td>
  </tr>

  <% end %>
</table>

<%= button_to 'Add New Treatment', new_treatment_path, method: :get %>
